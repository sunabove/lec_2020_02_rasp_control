   "source": [
    "from gpiozero import LED\n",
    "from time import sleep\n",
    "\n",
    "led = LED(17)\n",
    "try : \n",
    "    while True:\n",
    "        led.on()\n",
    "        sleep(1)\n",
    "        led.off()\n",
    "        sleep(1)\n",
    "        print( \"Running ....\" )\n",
    "    pass\n",
    "except KeyboardInterrupt :\n",
    "    led.off()\n",
    "    print( \"\\nStopped.\")\n",
    "pass"
   ]
